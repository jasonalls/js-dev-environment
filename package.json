{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript Development Environment Starter Kit",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain yewtree",
    "share": "npm-run-all --parallel security-check open:src localtunnel",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "test:lint:watch": "npm-run-all --parallel test:watch lint:watch",
    "test:security-check": "npm-run-all --parallel test security-check",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001"
  },
  "author": "Jason Alls",
  "license": "MIT",
  "dependencies": {
    "babel-preset-latest": "^6.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "browsersync": "^0.0.1-security",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "chance": "^1.0.12",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "json-schema-faker": "^0.4.6",
    "json-server": "^0.12.1",
    "localtunnel": "^1.8.3",
    "mocha": "^4.0.1",
    "nock": "^9.1.5",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "randexp": "^0.4.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-md5-hash": "^0.0.6",
    "whatwg-fetch": "^2.0.3"
  }
}
