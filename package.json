    "connect": "^3.6.5",
    "console": "^0.6.4",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "delete": "^1.1.0",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "exorcist": "^1.0.0",
    "express": "^4.16.2",
